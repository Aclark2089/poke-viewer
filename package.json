{
  "name": "poke-viewer",
  "version": "1.0.0",
  "description": "Simple Application to View Pokemon",
  "engines": {
    "node": "9.11.1"
  },
  "main": "index.js",
  "scripts": {
    "serve": "node dist/server.js",
    "start": "nodemon --inspect index.js --exec babel-node",
    "test": "jest -c jest.config.js",
    "test:int": "jest -c jest.config.integration.js",
    "build": "rimraf dist/ && babel src -d dist",
    "build:prod": "cross-env BABEL_ENV=production npm run build",
    "lint": "eslint src/"
  },
  "keywords": [
    "GraphQL",
    "Apollo",
    "Server",
    "Pokemon"
  ],
  "author": "R. Alex Clark",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "eslint": "^5.8.0",
    "eslint-plugin-jest": "^22.3.0",
    "graphql-query-generator": "^0.5.5",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "nodemon": "^1.18.6",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "apollo-server-express": "^2.1.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.3"
  }
}
